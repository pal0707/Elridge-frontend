{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { reactive, computed } from \"vue\";\nimport useValidate from \"@vuelidate/core\";\nimport { required, email, minLength } from \"@vuelidate/validators\";\nimport axios from \"axios\";\nexport default {\n  name: \"Login\",\n  components: {},\n  setup() {\n    const state = reactive({\n      email: \"\",\n      password: \"\"\n    });\n    const rules = computed(() => {\n      return {\n        email: {\n          required,\n          email\n        },\n        password: {\n          required,\n          minLength: minLength(6)\n        }\n      };\n    });\n    const v$ = useValidate(rules, state);\n    return {\n      state,\n      v$\n    };\n  },\n  data() {\n    return {\n      email: \"\",\n      password: \"\"\n    };\n  },\n  validations() {\n    return {\n      email: {\n        required\n      },\n      password: {\n        required\n      }\n    };\n  },\n  methods: {\n    submitForm() {\n      this.v$.$validate(); // checks all inputs\n      if (!this.v$.$error) {\n        axios.post(`http://127.0.0.1:8000/login/`, this.state).then(response => {\n          console.log(response);\n          this.$router.push(\"/about\");\n        }).catch(error => {\n          console.log(error);\n        });\n        alert(\"Login successfully.\");\n      } else {\n        alert(\"Form failed validation\");\n      }\n    },\n    goTo() {\n      this.$router.back();\n    }\n  }\n};","map":{"version":3,"names":["reactive","computed","useValidate","required","email","minLength","axios","name","components","setup","state","password","rules","v$","data","validations","methods","submitForm","$validate","$error","post","then","response","console","log","$router","push","catch","error","alert","goTo","back"],"sources":["/home/vikas/deepak/myapp/src/views/Login.vue"],"sourcesContent":["<template>\n  <div class=\"root\">\n    <h2>Login</h2>\n    <p>\n      <input type=\"text\" placeholder=\"Email\" v-model=\"state.email\" />\n      <span v-if=\"v$.email.$error\">\n        {{ v$.email.$errors[0].$message }}\n      </span>\n    </p>\n    <p>\n      <input type=\"password\" placeholder=\"Password\" v-model=\"state.password\" />\n      <span v-if=\"v$.password.$error\">\n        {{ v$.password.$errors[0].$message }}\n      </span>\n    </p>\n    <button @click=\"submitForm\">Submit</button>\n  </div>\n</template>\n\n<script>\nimport { reactive, computed } from \"vue\";\nimport useValidate from \"@vuelidate/core\";\nimport { required, email, minLength } from \"@vuelidate/validators\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"Login\",\n  components: {},\n  setup() {\n    const state = reactive({\n      email: \"\",\n      password: \"\",\n    });\n\n    const rules = computed(() => {\n      return {\n        email: {\n          required,\n          email,\n        },\n        password: {\n          required,\n          minLength: minLength(6),\n        },\n      };\n    });\n    const v$ = useValidate(rules, state);\n    return { state, v$ };\n  },\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n    };\n  },\n  validations() {\n    return {\n      email: { required },\n      password: { required },\n    };\n  },\n  methods: {\n    submitForm() {\n      this.v$.$validate(); // checks all inputs\n      if (!this.v$.$error) {\n        axios\n          .post(`http://127.0.0.1:8000/login/`, this.state)\n          .then((response) => {\n            console.log(response)\n            this.$router.push(\"/about\");\n          })\n          .catch((error) => {\n            console.log(error);\n          });\n        alert(\"Login successfully.\");\n      } else {\n        alert(\"Form failed validation\");\n      }\n    },\n    goTo() {\n      this.$router.back();\n    },\n  },\n};\n</script>\n\n<style lang=\"css\">\n.root {\n  width: 400px;\n  margin: 0 auto;\n  background-color: #fff;\n  padding: 30px;\n  margin-top: 100px;\n  border-radius: 2