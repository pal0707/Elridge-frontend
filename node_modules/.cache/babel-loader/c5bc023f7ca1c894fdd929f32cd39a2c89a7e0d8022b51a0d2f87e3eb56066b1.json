{"ast":null,"code":"import { reactive, computed } from \"vue\";\nimport useValidate from \"@vuelidate/core\";\nimport { required, email, minLength, sameAs } from \"@vuelidate/validators\";\nexport default {\n  name: \"SignUp\",\n  components: {},\n  setup() {\n    const state = reactive({\n      email: \"\",\n      password: {\n        password: \"\",\n        confirm: \"\"\n      }\n    });\n    const rules = computed(() => {\n      return {\n        email: {\n          required,\n          email\n        },\n        password: {\n          password: {\n            required,\n            minLength: minLength(6)\n          },\n          confirm: {\n            required,\n            sameAs: sameAs(state.password.password)\n          }\n        }\n      };\n    });\n    const v$ = useValidate(rules, state);\n    return {\n      state,\n      v$\n    };\n  },\n  data() {\n    return {\n      //   v$: useValidate(),\n      email: \"\",\n      password: {\n        password: \"\",\n        confirm: \"\"\n      }\n    };\n  },\n  validations() {\n    return {\n      email: {\n        required\n      },\n      password: {\n        password: {\n          required\n        },\n        confirm: {\n          required\n        }\n      }\n    };\n  },\n  methods: {\n    submitForm() {\n      this.v$.$validate(); // checks all inputs\n      if (!this.v$.$error) {\n        localStorage.setItem(\"userData\", JSON.stringify(this.state));\n        alert(\"Form successfully submitted.\");\n      } else {\n        alert(\"Form failed validation\");\n      }\n    },\n    goTo() {\n      this.$router.back();\n    }\n  }\n};","map":{"version":3,"names":["reactive","computed","useValidate","required","email","minLength","sameAs","name","components","setup","state","password","confirm","rules","v$","data","validations","methods","submitForm","$validate","$error","localStorage","setItem","JSON","stringify","alert","goTo","$router","back"],"sources":["/home/vikas/deepak/myapp/src/views/SignUp.vue"],"sourcesContent":["<template>\n  <div class=\"root\">\n    <h2>Create an Account</h2>\n    <p>\n      <input type=\"text\" placeholder=\"Email\" v-model=\"state.email\" />\n      <span v-if=\"v$.email.$error\">\n        {{ v$.email.$errors[0].$message }}\n      </span>\n    </p>\n    <p>\n      <input\n        type=\"password\"\n        placeholder=\"Password\"\n        v-model=\"state.password.password\"\n      />\n      <span v-if=\"v$.password.password.$error\">\n        {{ v$.password.password.$errors[0].$message }}\n      </span>\n    </p>\n    <p>\n      <input\n        type=\"password\"\n        placeholder=\"Confirm Password\"\n        v-model=\"state.password.confirm\"\n      />\n      <span v-if=\"v$.password.confirm.$error\">\n        {{ v$.password.confirm.$errors[0].$message }}\n      </span>\n    </p>\n    <button @click=\"submitForm\">Submit</button>\n  </div>\n</template>\n\n<script>\nimport { reactive, computed } from \"vue\";\nimport useValidate from \"@vuelidate/core\";\nimport { required, email, minLength, sameAs } from \"@vuelidate/validators\";\nexport default {\n  name: \"SignUp\",\n  components: {},\n  setup() {\n    const state = reactive({\n      email: \"\",\n      password: {\n        password: \"\",\n        confirm: \"\",\n      },\n    });\n\n    const rules = computed(() => {\n      return {\n        email: {\n          required,\n          email,\n        },\n        password: {\n          password: { required, minLength: minLength(6) },\n          confirm: { required, sameAs: sameAs(state.password.password) },\n        },\n      };\n    });\n    const v$ = useValidate(rules, state);\n    return { state, v$ };\n  },\n  data() {\n    return {\n      //   v$: useValidate(),\n      email: \"\",\n      password: {\n        password: \"\",\n        confirm: \"\",\n      },\n    };\n  },\n  validations() {\n    return {\n      email: { required },\n      password: {\n        password: { required },\n        confirm: { required },\n      },\n    };\n  },\n  methods: {\n    submitForm() {\n      this.v$.$validate(); // checks all inputs\n      if (!this.v$.$error) {\n        localStorage.setItem(\"userData\", JSON.stringify(this.state));\n        alert(\"Form successfully submitted.\");\n      } else {\n        alert(\"Form failed validation\");\n      }\n    },\n    goTo() {\n      this.$router.back();\n    },\n  },\n};\n</script>\n\n<style lang=\"css\">\n.root {\n  width: 400px;\n  margin: 0 auto;\n  background-color: #fff;\n  padding: 30px;\n  margin-top: 100px;\n  border-radius: 20px;\n}\ninput {\n  border: none;\n  outline: none;\n  border-bottom: 1px solid #ddd;\n  font-size: 1em;\n  padding: 5px 0;\n  margin: 10px 0 5px 0;\n  width: 100%;\n}\nbutton {\n  background-color: #3498db;\n  padding: 10px 20px;\n  margin-top: 10px;\n  border: none;\n  color: white;\n}\n</style>\n"],"mappings":"AAkCA,SAASA,QAAQ,EAAEC,QAAO,QAAS,KAAK;AACxC,OAAOC,WAAU,MAAO,iBAAiB;AACzC,SAASC,QAAQ,EAAEC,KAAK,EAAEC,SAAS,EAAEC,MAAK,QAAS,uBAAuB;AAC1E,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE,CAAC,CAAC;EACdC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIV,QAAQ,CAAC;MACrBI,KAAK,EAAE,EAAE;MACTO,QAAQ,EAAE;QACRA,QAAQ,EAAE,EAAE;QACZC,OAAO,EAAE;MACX;IACF,CAAC,CAAC;IAEF,MAAMC,KAAI,GAAIZ,QAAQ,CAAC,MAAM;MAC3B,OAAO;QACLG,KAAK,EAAE;UACLD,QAAQ;UACRC;QACF,CAAC;QACDO,QAAQ,EAAE;UACRA,QAAQ,EAAE;YAAER,QAAQ;YAAEE,SAAS,EAAEA,SAAS,CAAC,CAAC;UAAE,CAAC;UAC/CO,OAAO,EAAE;YAAET,QAAQ;YAAEG,MAAM,EAAEA,MAAM,CAACI,KAAK,CAACC,QAAQ,CAACA,QAAQ;UAAE;QAC/D;MACF,CAAC;IACH,CAAC,CAAC;IACF,MAAMG,EAAC,GAAIZ,WAAW,CAACW,KAAK,EAAEH,KAAK,CAAC;IACpC,OAAO;MAAEA,KAAK;MAAEI;IAAG,CAAC;EACtB,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACL;MACAX,KAAK,EAAE,EAAE;MACTO,QAAQ,EAAE;QACRA,QAAQ,EAAE,EAAE;QACZC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDI,WAAWA,CAAA,EAAG;IACZ,OAAO;MACLZ,KAAK,EAAE;QAAED;MAAS,CAAC;MACnBQ,QAAQ,EAAE;QACRA,QAAQ,EAAE;UAAER;QAAS,CAAC;QACtBS,OAAO,EAAE;UAAET;QAAS;MACtB;IACF,CAAC;EACH,CAAC;EACDc,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACJ,EAAE,CAACK,SAAS,EAAE,EAAE;MACrB,I